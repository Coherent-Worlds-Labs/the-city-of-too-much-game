# Runtime
NODE_ENV=development
APP_PORT=3000

# Storage
DATABASE_URL=file:./data/the-city-of-too-much.db
ASSETS_DIR=./storage/images
PUBLIC_ASSETS_BASE_URL=http://localhost:3000/assets
WORLD_PACK_FILE=the-city-of-too-much.en.json

# OpenRouter
OPENROUTER_API_KEY=replace-me
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_DEBUG=false
JUDGE_MODEL=openai/gpt-5-mini
IMAGE_MODEL=openai/gpt-5-image-mini
IMAGE_ASPECT_RATIO=21:9
IMAGE_OUTPUT_SIZE=672x288
IMAGE_QUALITY=low
IMAGE_MODALITIES=image
IMAGE_MAX_COMPLETION_TOKENS=
IMAGE_REASONING_EFFORT=low
IMAGE_PROMPT_MAX_CHARS=460
IMAGE_TO_IMAGE_ENABLED=false

# Reliability
MAX_HISTORY_ENTRIES=120
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30
